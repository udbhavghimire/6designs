RewriteEngine On

# Redirect all city requests (except Toronto) to cities handler
RedirectMatch 301 ^/([^/]+)-idx-website-for-realtors/?$ /idx-website-for-realtors/cities/index.php?city=$1

# Prevent direct access to template files
<FilesMatch "(city-template|cities)\.php$">
    Order Deny,Allow
    Deny from all
</FilesMatch>